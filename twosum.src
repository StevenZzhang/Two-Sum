class Solution {
    public int[] twoSum(int[] nums, int target) {
        int total = nums.length;
        for (int i = 0; i < total; i++) {
            for (int j = i + 1; j < total; j++) {
                if (nums[i] + nums[j] == target) {
                    int[] fin = new int[2];
                    fin[0] = i;
                    fin[1] = j;
                    return (fin);
                }
            }
        }
        return null;
    }
}
